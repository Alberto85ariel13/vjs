{
    "name": "vts-helper",

    "version": "0.1.31",
    "description": "Vanilla Typescript Helper for Functional Programming",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "/lib",
        "README.md"
    ],

    "scripts": {
        "test": "nyc mocha --opts ./tests/mocha.opts tests/**/*.spec.ts",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
